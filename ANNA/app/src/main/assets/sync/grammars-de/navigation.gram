<postcode> = (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null) (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null) (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null) (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null) (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null );
<number> = (eins | zwei | drei | vier | fünf | sechs | sieben | acht | neun | null) *;
<street> = (a | b | c | de | e | f | g | ha | i | j | k | l | m | n | o | p | q | r | s | t | u | v | w | x | y | z) *;